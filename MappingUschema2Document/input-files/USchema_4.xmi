<?xml version="1.0" encoding="ASCII"?>
<uschema:USchema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:uschema="http://www.modelum.es/uschema"
    name="Biblioteca">
  <entities
      name="Autor"
      root="true">
    <features
        xsi:type="uschema:Attribute"
        name="Nombre">
      <type
          xsi:type="uschema:PrimitiveType"
          name="String"/>
    </features>
    <features
        xsi:type="uschema:Attribute"
        name="EditorialesFavoritas">
      <type
          xsi:type="uschema:PSet">
        <elementType
            xsi:type="uschema:PrimitiveType"
            name="String"/>
      </type>
    </features>
    <features
        xsi:type="uschema:Attribute"
        name="AutorID"
        key="//@entities.0/@features.3">
      <type
          xsi:type="uschema:PrimitiveType"
          name="int"/>
    </features>
    <features
        xsi:type="uschema:Key"
        name="Autor_PK"
        attributes="//@entities.0/@features.2"/>
  </entities>
  <entities
      name="Libro"
      root="true">
    <features
        xsi:type="uschema:Attribute"
        name="LibroID"
        key="//@entities.1/@features.3">
      <type
          xsi:type="uschema:PrimitiveType"
          name="int"/>
    </features>
    <features
        xsi:type="uschema:Attribute"
        name="ISBN"
        key="//@entities.1/@features.3">
      <type
          xsi:type="uschema:PrimitiveType"
          name="String"/>
    </features>
    <features
        xsi:type="uschema:Attribute"
        name="Titulo">
      <type
          xsi:type="uschema:PrimitiveType"
          name="String"/>
    </features>
    <features
        xsi:type="uschema:Key"
        name="Libro_PK"
        attributes="//@entities.1/@features.0 //@entities.1/@features.1"/>
  </entities>
</uschema:USchema>
