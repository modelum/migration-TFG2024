<?xml version="1.0" encoding="UTF-8"?>
<relationalschema:RelationalSchema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:relationalschema="http://www.example.org/relationalschema"
    name="Tienda"
    location="">
  <tables name="Cliente">
    <columns
        name="Nombre"
        datatype="VARCHAR"
        size="20"/>
    <columns
        name="Apellido"
        datatype="VARCHAR"
        size="20"/>
    <columns
        name="Telefono"
        datatype="INT"
        size="9"/>
    <keys columns="//@tables.0/@columns.0 //@tables.0/@columns.1 //@tables.0/@columns.2"
        constraintname="ID_cliente"/>
  </tables>
  <tables name="Producto">
    <columns
        name="NombreProducto"
        datatype="VARCHAR"
        size="20"/>
    <columns
        name="Descripcion"
        datatype="VARCHAR"
        size="200"/>
    <columns
        name="Precio"
        datatype="INT"
        size="4"
        nullable="true"/>
    <columns
        name="Stock"
        datatype="INT"
        size="4"/>
    <keys columns="//@tables.1/@columns.3"
        constraintname="ID_uk_pedido"
        isPK="false"/>
    <keys columns="//@tables.1/@columns.0 //@tables.1/@columns.1"
        constraintname="ID_pedido"/>
  </tables>
</relationalschema:RelationalSchema>
