<?xml version="1.0" encoding="UTF-8"?>
<relationalschema:RelationalSchema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:relationalschema="http://www.example.org/relationalschema"
    name="Biblioteca">
  <tables name="Autor">
    <columns
        name="Nombre"
        datatype="varchar"/>
    <columns
        name="FechaNacimiento"
        datatype="DATE"/>
    <columns
        name="AutorID"
        datatype="INT"/>
    <keys columns="//@tables.0/@columns.2"
        constraintname="Autor_PK"/>
  </tables>
  <tables name="Libro">
    <columns
        name="LibroID"
        datatype="INT"/>
    <columns
        name="Titulo"
        datatype="VARCHAR"/>
    <columns
        name="AÃ±oPublicacion"
        datatype="INT"/>
    <columns
        name="ISBN"
        datatype="VARCHAR"/>
    <keys columns="//@tables.1/@columns.0"
        constraintname="Libro_PK"/>
    <keys columns="//@tables.1/@columns.3"
        constraintname="ISBN_UK"
        isPK="false"/>
  </tables>
  <tables name="Libros_Autores">
    <columns
        name="Libro"
        datatype="INT"/>
    <columns
        name="Autor"
        datatype="INT"/>
    <keys columns="//@tables.2/@columns.0 //@tables.2/@columns.1"
        constraintname="Libros_Autores_PK"/>
    <fks columns="//@tables.2/@columns.0"
        refsTo="//@tables.1/@keys.0"
        constraintname="Libro_FK"/>
    <fks columns="//@tables.2/@columns.1"
        refsTo="//@tables.0/@keys.0"
        constraintname="Autor_FK"/>
  </tables>
</relationalschema:RelationalSchema>
