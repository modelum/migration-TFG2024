<?xml version="1.0" encoding="UTF-8"?>
<relationalschema:RelationalSchema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:relationalschema="http://www.example.org/relationalschema"
    name="Biblioteca">
  <tables name="Libro">
    <columns
        name="LibroID"
        datatype="INT"/>
    <keys columns="//@tables.0/@columns.0"
        constraintname="Libro_PK"/>
  </tables>
  <tables name="Reserva">
    <columns
        name="ReservaID"
        datatype="INT"/>
    <columns
        name="Libro"
        datatype="INT"/>
    <keys columns="//@tables.1/@columns.0"
        constraintname="Reserva_PK"/>
    <keys columns="//@tables.1/@columns.1"
        constraintname="Libro_UK"
        isPK="false"/>
    <fks columns="//@tables.1/@columns.1"
        refsTo="//@tables.0/@keys.0"
        constraintname="Libro_FK"/>
  </tables>
</relationalschema:RelationalSchema>
