<?xml version="1.0" encoding="UTF-8"?>
<documentschema:DocumentSchema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:documentschema="http://www.example.org/documentschema"
    name="Metashop">
  <entities
      name="User">
    <properties
        xsi:type="documentschema:Attribute"
        name="address"
        type="//@types.0"/>
    <properties
        xsi:type="documentschema:Attribute"
        name="phone"
        type="//@types.1"/>
    <properties
        xsi:type="documentschema:Attribute"
        name="id"
        isKey="true"
        type="//@types.2"/>
  </entities>
  <entities
      name="Shop">
    <properties
        xsi:type="documentschema:Attribute"
        name="address"
        type="//@types.3"/>
    <properties
        xsi:type="documentschema:Attribute"
        name="id"
        isKey="true"
        type="//@types.4"/>
  </entities>
  <types xsi:type="documentschema:PrimitiveType"/>
  <types xsi:type="documentschema:PrimitiveType"
      datatype="INTEGER"/>
  <types xsi:type="documentschema:PrimitiveType"
      datatype="DOUBLE"/>
  <types xsi:type="documentschema:PrimitiveType"
      datatype="STRING"/>
  <types xsi:type="documentschema:Array"
      type="//@types.1"/>
</documentschema:DocumentSchema>
